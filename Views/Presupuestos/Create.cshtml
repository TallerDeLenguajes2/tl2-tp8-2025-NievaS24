@using tl2_tp8_2025_NievaS24.Models;
@model Presupuestos


<form asp-controller="Presupuestos" asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="NombreDestinatario"></label>
        <input asp-for="NombreDestinatario" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="FechaCreacion"></label>
        <input asp-for="FechaCreacion" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary mt-4">Crear</button>
</form>